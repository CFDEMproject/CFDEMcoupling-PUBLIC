FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          liggghtsCommands;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

liggghtsCommandModels
(
    runLiggghts
    writeLiggghts
);

runLiggghtsProps
{
    preNo false;
}

//- optional
writeLiggghtsProps
{
    writeLast off;
    writeName "post/restart/liggghts.restartCFDEM";
    overwrite on;
}

