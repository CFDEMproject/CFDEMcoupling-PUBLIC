FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    wall
    {
//        type            fixedFluxPressure;
//        phi             phi;
        type            zeroGradient;
    }

    inlet
    {
//        type            fixedFluxPressure;
//        phi             phi;
        type            zeroGradient;
    }
    outlet
    {
        //type            zeroGradient;
        type            fixedValue;
        value           uniform 0;
    }
}

