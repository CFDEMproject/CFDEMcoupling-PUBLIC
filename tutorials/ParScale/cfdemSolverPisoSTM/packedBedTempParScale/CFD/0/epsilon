FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.003;

boundaryField
{
    wall
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0.003;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.003;
        value           uniform 0.003;
    }
    inlet
    {
        /*type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.005;
        value           uniform 0.003;*/
        type            zeroGradient;
    }
}


