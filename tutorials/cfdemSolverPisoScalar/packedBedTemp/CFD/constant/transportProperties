FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

// Laminar viscosity
nu              nu [ 0 2 -1 0 0 0 0 ] 1.5e-03;

// Laminar Prandtl number
Pr              [0 0 0 0 0 0 0] 70.097; // alpha=nu/Pr=2.1399e-5

// Turbulent Prandtl number
Prt             [0 0 0 0 0 0 0] 1e15;


